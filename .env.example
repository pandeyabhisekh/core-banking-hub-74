# Core Banking System - Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api

# Application Configuration
VITE_APP_NAME=CoreBank CBS
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_NFT=true
VITE_ENABLE_FOREX=true
VITE_ENABLE_SIP=true

# Session Configuration
VITE_SESSION_TIMEOUT=1800000
# 30 minutes in milliseconds

# Banking Configuration
VITE_MAX_TRANSACTION_AMOUNT=1000000
VITE_NEFT_MIN_AMOUNT=1
VITE_RTGS_MIN_AMOUNT=200000
VITE_IMPS_MAX_AMOUNT=200000
